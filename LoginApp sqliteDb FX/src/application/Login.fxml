<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Label fx:id="isConnected" layoutX="58.0" layoutY="38.0" prefHeight="60.0" prefWidth="317.0" text="Status:" textFill="#f80d0d">
         <font>
            <Font name="Jokerman" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="password" layoutX="94.0" layoutY="282.0" promptText="password">
         <font>
            <Font name="Jokerman" size="18.0" />
         </font></PasswordField>
      <TextField fx:id="username" layoutX="94.0" layoutY="167.0" promptText="Username">
         <font>
            <Font name="Jokerman" size="18.0" />
         </font></TextField>
      <Button layoutX="178.0" layoutY="365.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font name="Jokerman" size="18.0" />
         </font></Button>
   </children>
</AnchorPane>
